<video
    id="{{_id}}"
    src="{{src}}" 
    class="{{className}}"
    controls="{{controls}}"
    autoplay="{{autoPlay}}"
    style="{{styleSheet}}" 
    loop="{{loop}}"
    poster="{{poster}}"
    show-fullscreen-btn="{{showFullscreenBtn}}"
    show-play-btn="{{showPlayBtn}}"
    show-mute-btn="{{showMuteBtn}}"
    show-center-play-btn="{{showCenterPlayBtn}}"
    bindtap="onClick"
    bindended="onEnded">
</video>
